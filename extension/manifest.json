{
  "manifest_version": 3,
  "name": "xtouch lite provisioning portal",
  "version": "1.1",
  "permissions": ["cookies", "activeTab", "tabs", "storage", "scripting"],
  "host_permissions": ["*://*/*"],
  "background": { "service_worker": "background.js" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": { "16": "icon.png", "48": "icon.png", "128": "icon.png" }
  }
}
